        <assemblyIdentity name="System.Threading.Tasks.Extensions" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.1.1.0" newVersion="4.1.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="DataBaseServiceSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:51031/Service/DataBaseService.asmx"
        binding="basicHttpBinding" bindingConfiguration="DataBaseServiceSoap"
        contract="DataBaseServiceReference.DataBaseServiceSoap" name="DataBaseServiceSoap" />
    </client>
